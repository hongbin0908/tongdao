<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" 
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/mission_diet_main_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:text="您創建了自定义任務"
        android:textColor="#FF0000"
        android:textSize="15pt" >
    </TextView>
     <com.tongdao.component.EditTextWithLabel 
        android:id="@+id/mission_diet_main_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    />
    <com.tongdao.component.EditTextWithLabel 
        android:id="@+id/mission_diet_main_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    />
    
    <com.tongdao.component.EditTextWithLabel 
        android:id="@+id/mission_diet_main_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    />
    
    <com.tongdao.component.EditTextWithLabel 
        android:id="@+id/mission_diet_main_motto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    />
    
     <com.tongdao.component.EditTextWithLabel 
        android:id="@+id/mission_diet_main_cash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    />
     
    <Button
        android:id="@+id/mission_diet_mian_submmit"
        android:text="创建任务"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    />
    


</LinearLayout>